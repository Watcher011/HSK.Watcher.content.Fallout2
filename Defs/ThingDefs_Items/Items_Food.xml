<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  

  <ThingDef ParentName="MealBase">
    <defName>SuspiciousOmelette</defName>
    <label>suspicious omelette</label>
    <description>A suspicious omelette made from the meat and eggs of a huge lizard. A favorite food in the wastelands</description>
    <graphicData>
      <texPath>Things/Item/Meal/SuspiciousOmelette</texPath>
     <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
      <DeteriorationRate>0.25</DeteriorationRate>
      <MarketValue>24</MarketValue>
      <Mass>0.3</Mass>
      <WorkToMake>450</WorkToMake>
      <Nutrition>0.9</Nutrition>
    </statBases>
    <ingestible>
      <preferability>MealSimple</preferability>
      <optimalityOffsetHumanlikes>-5</optimalityOffsetHumanlikes>
      <optimalityOffsetFeedingAnimals>-10</optimalityOffsetFeedingAnimals>
      <ingestEffect>EatVegetarian</ingestEffect>
      <ingestSound>Meal_Eat</ingestSound>
	  <tasteThought>RadFood</tasteThought>
	  <outcomeDoers>
				<li Class="IngestionOutcomeDoer_GiveHediff">
					<hediffDef>RimatomicsRadiation</hediffDef>
					<severity>0.06</severity>
				</li>
	   </outcomeDoers>
    </ingestible>
    <allowedArchonexusCount>40</allowedArchonexusCount>
	<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>5</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
  </ThingDef>


