<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="SK_DoorBase">
		<defName>VaultTecSecureLok</defName>
		<label>Vault-Tec  SecureLok</label>
		<description>Your reliable barrier between you and what's left of your neighbors! Includes: one rotary mechanism, one latch and a reminder that personal space is not a right, but a privilege granted by the Manager.</description>
		<statBases>
			<MaxHitPoints>350</MaxHitPoints>
			<WorkToBuild>800</WorkToBuild>
		</statBases>
		<graphicData>
			<texPath>Things/Building/Other/DoorFal_Mover</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<damageData>
				<!--  no damage marks because they don't move with the door
        <rect>(0,0.12,1,0.76)</rect>-->
				<enabled>false</enabled>
			</damageData>
		</graphicData>
		<uiIconPath>Things/Building/Other/DoorFal_Icon</uiIconPath>
		<stuffCategories>
			<li>RuggedMetallic</li>
		</stuffCategories>
		<costStuffCount>55</costStuffCount>
		<costList>
			<ComponentIndustrial>2</ComponentIndustrial>
			<ElectronicComponents>2</ElectronicComponents>
		</costList>
		<tickerType>Normal</tickerType>
		<comps>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>100</basePowerConsumption>
			</li>
			<li Class="CompProperties_Glower">
				<glowRadius>2</glowRadius>
				<glowColor>(217,177,113,0)</glowColor>
			</li>
			<li Class="CompProperties_Breakdownable"/>
			<li Class="SK.CompProperties_LowIdleDraw">
				<compClass>SK.CompPowerLowIdleDraw</compClass>
				<idlePowerFactor>0.1</idlePowerFactor>
				<operationalMode>WhenNear</operationalMode>
			</li>
			<li Class="CompProperties_Styleable" />
		</comps>
		<designationHotKey>Misc3</designationHotKey>
		<researchPrerequisites>
			<li>Autodoors</li>
		</researchPrerequisites>
		<constructionSkillPrerequisite>5</constructionSkillPrerequisite>
	</ThingDef>

