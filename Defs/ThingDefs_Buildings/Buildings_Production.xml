			<li Class="CompProperties_AffectedByFacilities">
				<linkableFacilities>
					<li>Small_ToolCabinet</li>
					<li>PodChair</li>
				</linkableFacilities>
			</li>
			<li Class="CompProperties_Refuelable">
				<fuelConsumptionRate>0.01</fuelConsumptionRate>
				<fuelCapacity>1.0</fuelCapacity>
				<fuelFilter>
					<thingDefs>
						<li>NuclearBattery</li>
