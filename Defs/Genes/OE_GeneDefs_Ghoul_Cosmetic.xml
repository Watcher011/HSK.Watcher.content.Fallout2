<Defs>
<!--=============== Skin - GhoulDarkGreen ====================-->
  <GeneDef ParentName="GeneSkinColorOverride">
    <defName>OE_Skin_GhoulDarkGreen</defName>
    <label>Ghoul's painfully pale skin</label>
    <labelShortAdj>Ghoul's skin is green</labelShortAdj>
    <description>The skin of a ghoul that has seen a lot.</description>
    <skinColorOverride>(87, 108, 89)</skinColorOverride>
    <displayOrderInCategory>713</displayOrderInCategory>
    <symbolPack>
      <prefixSymbols>
        <li><symbol>green</symbol></li>
        <li><symbol>ghoul</symbol></li>
      </prefixSymbols>
    </symbolPack>
  </GeneDef>
